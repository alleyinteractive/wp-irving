{
  "name": "irving/post-byline",
  "_alias": "irving/byline",
  "description": "Display a list of content authors.",
  "config": {
    "single_delimiter": {
      "default": " and ",
      "type": "string"
    },
    "multi_delimiter": {
      "default": ", ",
      "type": "string"
    },
    "last_delimiter": {
      "default": ", and ",
      "type": "string"
    },
    "post_id": {
      "type": "integer"
    },
    "pre_text": {
      "default": "By ",
      "type": "string"
    }
  },
  "use_context": {
    "irving/post_id": "post_id"
  }
}
