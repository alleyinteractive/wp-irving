{
  "name": "irving/query-pagination",
  "_alias": "irving/pagination",
  "description": "Pagination UI for a query.",
  "config": {
    "base_url": {
      "default": "/",
      "type": "string"
    },
    "current_page": {
      "default": 1,
      "type": "int"
    },
    "display_first_and_last": {
      "default": true,
      "type": "bool"
    },
    "display_prev_and_next": {
      "default": true,
      "type": "bool"
    },
    "pagination_format": {
      "default": "page/%1$d/",
      "type": "string"
    },
    "range": {
      "default": 5,
      "type": "number"
    },
    "style": {
      "default": [],
      "type": "object"
    },
    "total_pages": {
      "default": 1,
      "type": "int"
    },
    "wp_query": {
      "hidden": true,
      "type": "object"
    }
  },
  "use_context": {
    "irving/wp_query": "wp_query"
  }
}
