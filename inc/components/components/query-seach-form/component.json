{
  "name": "irving/query-search-form",
  "_alias": "irving/search-form",
  "description": "Form with an input field for search.",
  "config": {
    "base_url": {
      "default": "/",
      "type": "string"
    },
    "search_term": {
      "default": "",
      "description": "The string being searched for.",
      "type": "string"
    },
    "search_term_query_arg": {
      "default": "s",
      "description": "The query var used for the search term.",
      "type": "string"
    },
    "style": {
      "default": [],
      "required": false,
      "type": "array"
    },
    "wp_query": {
      "hidden": true,
      "type": "object"
    }
  },
  "use_context": {
    "irving/wp_query": "wp_query"
  }
}
