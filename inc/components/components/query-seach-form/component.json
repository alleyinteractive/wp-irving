{
  "name": "irving/query-search-form",
  "_alias": "irving/search-form",
  "description": "Form with an input field for search.",
  "config": {
    "base_url": {
      "default": "/",
      "type": "string"
    },
    "search_term": {
      "default": "",
      "type": "string"
    },
    "search_term_query_arg": {
      "default": "s",
      "type": "string"
    },
    "wp_query": {
      "hidden": true,
      "type": "object"
    },
    "analytics": {
      "default": {
        "search": {
          "action":  "Search",
          "category": "Navigation"
        }
      },
      "description": "Analytics Data",
      "type": "object"
    }
  },
  "use_context": {
    "irving/wp_query": "wp_query"
  }
}
